@model IEnumerable<SkiersLab.Models.RaceInfo>

@{
    ViewData["Title"] = "AllResultsandSkiers";
}

<h1>AllResultsandSkiers</h1>
<h3>
    @ViewBag.RuntimeRes
</h3>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.date_race)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.distance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.country)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.city)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.time_min)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.time_sec)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fis_point)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.year)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.athlete)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.date_race)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.distance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.country)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.city)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.time_min)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.time_sec)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fis_point)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.year)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.athlete)
            </td>
            <td>
              
            </td>
        </tr>
}
    </tbody>
</table>
