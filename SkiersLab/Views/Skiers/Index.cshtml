@model IEnumerable<SkiersLab.Models.Skier>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.year)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sex)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.athlete)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.year)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.age)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sex)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.athlete)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.fis_code">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.fis_code">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.fis_code">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
